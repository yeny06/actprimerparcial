def salud():
  print ("Ingresar la altura del paciente en cm")
  altura= input()
  print ("Ingresar el peso anterior del paciente")
  peso1= float(input())
  print("Ingresar el nuevo peso del paciente")
  peso2= float(input())
  altura2= int(altura)**2
  IMC1= int(peso1)/ int(altura2)
  IMC2= int(peso2)/ int(altura2)
  print ("El Indice de Masa Corporal del paciente en la primera semana es:",IMC1)
  print("El Indice de  Masa Corporal del paciente en la semana dos es:",IMC2)


salud()
